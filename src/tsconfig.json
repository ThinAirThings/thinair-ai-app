{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "jsx": "react-jsx",
        "jsxImportSource": "@emotion/react",
        "outDir": "dist",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "strictNullChecks": true
    },
}